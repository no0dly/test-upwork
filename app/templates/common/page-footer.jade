footer.page-footer
	.page-footer__wrap
		.socials
			ul.socials__list
				each item in ['fb','tw','go','p']
					li.socials__item
						a(href="#" class="socials__link socials__link-" + item)= item
		nav.navigation
			ul.navigation__list
				each item in ['About Us', 'Privacy Policy', 'Terms & Conditions', 'Contact Us', 'Cookie Policy']
					li.navigation__item
						a.navigation__link(href="#")= item
